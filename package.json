{
  "name": "monaco-editor",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jmfirth.github.io/typescript-playground",
  "devDependencies": {
    "preact-scripts-ts": "0.9.7"
  },
  "dependencies": {
    "@types/lodash": "^4.14.59",
    "@types/lz-string": "^1.3.31",
    "@types/node": "^7.0.11",
    "gh-pages-deploy": "^0.4.2",
    "inline-style-prefixer": "^3.0.2",
    "lodash": "^4.17.4",
    "lz-string": "^1.4.4",
    "material-design-icons": "^3.0.1",
    "mdi": "^1.8.36",
    "monaco-editor": "^0.8.3",
    "preact": "^7.2.1",
    "rimraf": "^2.6.1",
    "source-map-support": "0.4.3"
  },
  "scripts": {
    "start": "preact-scripts-ts start",
    "build": "preact-scripts-ts build",
    "test": "preact-scripts-ts test --env=jsdom",
    "eject": "preact-scripts-ts eject",
    "lint": "tslint -c ./tslint.json \"src/**/*.ts\" \"src/**/*.tsx\" -e \"src/**/*.d.ts\"",
    "deploy": "gh-pages-deploy",
    "clean-source": "rimraf src public build package.json tsconfig.json tslint.json yarn.lock"
  },
  "gh-pages-deploy": {
    "staticpath": "build",
    "prep": [
      "build"
    ],
    "post": [
      "clean-source"
    ],
    "noprompt": false
  }
}
